<%- include("partials/header") %> 

<h2>Author Details</h2>

<% for(i=0;i<posts.length;i++){ %>
    <li> <%= posts[i].title %> : <%= posts[i].author %></li>
<% } %>

<% posts.forEach(function(posts){ %>
    <li>
        <%= posts.title %> - <strong><%= posts.author%></strong>
    </li>
<% }); %>
<%- include("partials/footer") %>